    <link name="rplidar_link">
      <pose>.05 0 .134 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.17</mass>
        <inertia>
          <ixx>0.0001926666667</ixx> <!-- (.17/12)*(.1^2+.06^2) -->
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001224141667</iyy> <!-- (.17/12)*(.071^2+.06^2) -->
          <iyz>0</iyz>
          <izz>0.0002130808333</izz> <!-- (.17/12)*(.071^2+.1^2) -->
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://mrbuggy3/meshes/rplidar.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <sensor name="rplidar" type="gpu_lidar">
        <lidar>
          <scan>
            <horizontal>
              <samples>400</samples>
              <resolution>1</resolution>
              <min_angle>-3.14159265359</min_angle>
              <max_angle>3.14159265359</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>12</max>
            <resolution>0.01</resolution>
          </range>
        </lidar>
        <gz_frame_id>rplidar_link</gz_frame_id>
        <alwaysOn>1</alwaysOn>
        <visualize>true</visualize>
        <update_rate>10</update_rate>
      </sensor>
    </link>

    <joint name="rplidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>rplidar_link</child>
    </joint>

    <joint name="FrontRightwheelSteeringJoint" type="revolute">
      <parent>base_link</parent>
      <child>FrontRightwheelSteering</child>
      <pose relative_to="base_link">.112 -.10 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.6</lower>
          <upper>0.6</upper>
          <velocity>1000</velocity>
          <effort>1000</effort>
        </limit>
      </axis>
    </joint>

    <joint name="FrontRightwheelJoint" type="revolute">
      <parent>FrontRightwheelSteering</parent>
      <child>FrontRightwheel</child>
      <pose relative_to="FrontRightwheelSteeringJoint">0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1.79769e+308</lower>
          <upper>1.79769e+308</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
